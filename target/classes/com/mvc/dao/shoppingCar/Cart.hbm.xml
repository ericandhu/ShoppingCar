<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mvc.dao">
	<class name="Cart" table="cart">
		<id name="cart_number" column="cart_number">
<!-- 			<generator class="native" /> -->
		</id>
		<property name="customer" column="customer" />
		<property name="amount" column="amount" />
		<property name="created_by" column="created_by" />
		<property name="created_date" column="created_date" />
		<property name="last_modified_by" column="last_modified_by" />
		<property name="last_modified_date" column="last_modified_date" />
	</class>
</hibernate-mapping>
